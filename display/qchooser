import csv
import datetime
import random
import os

dirloc = 'C:\\pve\\qd\\testfiles'
dfiles = ['skquotes','scquotes']

def qchooser(sdict):
    quotedict = {}
    seldict = {}
    randomseed()
    for item in dfiles:
        quotedict[item] = []
        x = 1
        with open(os.path.join(dirloc,item+'.csv'),newline = '') as csvfile:
            rawlines = csv.reader(csvfile, delimiter = ',')
            for row in rawlines:
                if row[0] == 'type' or row[0] == '' or row[0] == 'quote':
                    pass
                else:
                    quotedict[item].append(row)
        ts = datetime.datetime.now().microsecond*(sum(sdict[item])+1)
        print(ts)
        random.seed(ts)

    return quotedict

